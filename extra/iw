pkgbuild_version=5
pkgname=iw
pkgver=5.3

dependencies=(
    "!$pkgname"
	'libnl'
)
sources=(
	"https://cdn.kernel.org/pub/software/network/iw/${pkgname}-${pkgver}.tar.xz"
)

sha1sums=(
)

lfs_me_prepare() {
	tar -xf "${sources_dir}/${pkgname}-${pkgver}.tar.xz" -C "$build_dir"

	cd "${build_dir}/${pkgname}-${pkgver}"

}


lfs_me_install() {
	cd "${build_dir}/${pkgname}-${pkgver}"
	make DESTDIR="$fakeroot_dir" install
}

